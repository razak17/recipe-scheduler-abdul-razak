{
	"name": "recipe-scheduler",
	"version": "1.0.0",
	"scripts": {
		"build": "tsc",
		"start": "node build/server.js",
		"dev": "ts-node-dev --respawn --transpile-only src/server.ts",
		"debug": "nodemon --watch src --inspect=0.0.0.0:9229 --exec 'ts-node' src/server.ts",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand tests/integration",
		"test:e2e": "jest --config jest.e2e.config.js",
		"test:integration": "jest tests/integration",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"bcrypt": "^6.0.0",
		"bullmq": "^5.53.0",
		"cors": "^2.8.5",
		"dotenv": "^16.5.0",
		"expo-server-sdk": "^3.15.0",
		"express": "^5.1.0",
		"express-rate-limit": "^7.5.0",
		"helmet": "^8.1.0",
		"jsonwebtoken": "^9.0.2",
		"node-cron": "^4.0.6",
		"redis": "^4.7.1",
		"reflect-metadata": "^0.2.2",
		"sqlite3": "^5.1.7",
		"typeorm": "^0.3.24",
		"winston": "^3.17.0",
		"winston-daily-rotate-file": "^5.0.0",
		"zod": "^3.25.20"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.18",
		"@types/express": "^5.0.2",
		"@types/jest": "^29.5.14",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/node": "^22.15.21",
		"@types/node-cron": "^3.0.11",
		"@types/supertest": "^6.0.3",
		"@typescript-eslint/parser": "^8.32.1",
		"jest": "^29.7.0",
		"nodemon": "^3.1.10",
		"supertest": "^7.1.1",
		"ts-jest": "^29.3.4",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.8.3"
	}
}
